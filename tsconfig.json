{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "src/**/*",
        ".astro/**/*",
        "astro.config.mjs"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}